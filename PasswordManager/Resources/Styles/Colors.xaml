<?xml version="1.0" encoding="UTF-8" ?>
<ResourceDictionary
  x:Class="PasswordManager.Resources.Styles.Colors"
  xmlns="http://xamarin.com/schemas/2014/forms"
  xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">

  <!--  Default font color with grading from High emphasis, Medium emphasis to Disabled.  -->
  <Color x:Key="BlackHighEmphasisColor">#000000</Color>
  <Color x:Key="BlackMediumEmphasisColor">#535353</Color>
  <Color x:Key="WhiteHighEmphasisColor">#FFFFFF</Color>

  <!--  Highlight color for interactive elements.  -->
  <Color x:Key="PrimaryColor">#009FE3</Color>

  <Color x:Key="GrayScaleColor">#E2E2E2</Color>
  <Color x:Key="ErrorColor">#f5424b</Color>
</ResourceDictionary>
